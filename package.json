{
  "name": "dot-strive",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "bun run build && wrangler pages deploy ./public",
    "migrations:gen": "drizzle-kit generate:sqlite --config=./database/drizzle.config.ts",
    "migrations:drop": "drizzle-kit drop --config=./database/drizzle.config.ts",
    "migrations:apply": "wrangler d1 migrations apply dot-strive --local",
    "production:migrations:apply": "NO_D1_WARNING=true wrangler d1 migrations apply dot-strive"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@remix-run/cloudflare": "2.3.1",
    "@remix-run/cloudflare-pages": "2.3.1",
    "@remix-run/css-bundle": "2.3.1",
    "@remix-run/react": "2.3.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "drizzle-orm": "0.29.0",
    "isbot": "3.7.1",
    "lucide-react": "^0.293.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-auth": "3.6.0",
    "remix-auth-google": "2.0.0",
    "tailwind-merge": "2.0.0",
    "tailwindcss-animate": "1.0.7",
    "valibot": "0.21.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20231121.0",
    "@remix-run/dev": "2.3.1",
    "@remix-run/eslint-config": "2.3.1",
    "@tsconfig/strictest": "2.0.2",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.0",
    "autoprefixer": "10.4.16",
    "better-sqlite3": "9.1.1",
    "drizzle-kit": "0.20.6",
    "eslint": "8.54.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "markuplint": "3.14.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2",
    "wrangler": "3.17.1"
  },
  "engines": {
    "node": "20.10.0"
  }
}
