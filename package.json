{
  "name": "dot-strive",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "bun run build && wrangler pages deploy ./public"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.2.0",
    "@remix-run/cloudflare-pages": "2.2.0",
    "@remix-run/css-bundle": "2.2.0",
    "@remix-run/react": "2.2.0",
    "isbot": "3.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20231025.0",
    "@remix-run/dev": "2.2.0",
    "@remix-run/eslint-config": "2.2.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "eslint": "8.53.0",
    "typescript": "5.2.2",
    "wrangler": "3.1.1"
  },
  "engines": {
    "node": "18.0.0"
  }
}
