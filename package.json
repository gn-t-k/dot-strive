{
  "name": "dot-strive",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "bun run build && wrangler pages deploy ./public",
    "migrations:gen": "drizzle-kit generate:sqlite --config=./database/drizzle.config.ts",
    "migrations:drop": "drizzle-kit drop --config=./database/drizzle.config.ts",
    "migrations:apply": "wrangler d1 migrations apply dot-strive --local",
    "production:migrations:apply": "NO_D1_WARNING=true wrangler d1 migrations apply dot-strive"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@remix-run/cloudflare": "2.4.1",
    "@remix-run/cloudflare-pages": "2.4.1",
    "@remix-run/css-bundle": "2.4.1",
    "@remix-run/react": "2.4.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "drizzle-orm": "0.29.3",
    "isbot": "4.3.0",
    "lucide-react": "^0.306.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-auth": "3.6.0",
    "remix-auth-google": "2.0.0",
    "remix-validated-form": "^5.1.5",
    "tailwind-merge": "2.2.0",
    "tailwindcss-animate": "1.0.7",
    "valibot": "0.25.0",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20231218.0",
    "@remix-run/dev": "2.4.1",
    "@remix-run/eslint-config": "2.4.1",
    "@tsconfig/strictest": "2.0.2",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "autoprefixer": "10.4.16",
    "better-sqlite3": "9.2.2",
    "drizzle-kit": "0.20.10",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.13.1",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-unicorn": "50.0.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "markuplint": "3.15.0",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3",
    "wrangler": "3.22.3"
  },
  "engines": {
    "node": "20.10.0"
  }
}
