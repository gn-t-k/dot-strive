{
  "name": "dot-strive",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "bun run build && wrangler pages deploy ./public",
    "migrations:gen": "drizzle-kit generate:sqlite --config=./database/drizzle.config.ts",
    "migrations:drop": "drizzle-kit drop --config=./database/drizzle.config.ts",
    "migrations:apply": "wrangler d1 migrations apply dot-strive --local",
    "production:migrations:apply": "NO_D1_WARNING=true wrangler d1 migrations apply dot-strive"
  },
  "dependencies": {
    "@conform-to/react": "1.0.3",
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-alert-dialog": "1.0.5",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-visually-hidden": "1.0.3",
    "@remix-run/cloudflare": "2.8.1",
    "@remix-run/cloudflare-pages": "2.8.1",
    "@remix-run/css-bundle": "2.8.1",
    "@remix-run/react": "2.8.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "conform-to-valibot": "1.5.1",
    "drizzle-orm": "0.30.1",
    "isbot": "5.1.1",
    "lucide-react": "0.354.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-auth": "3.6.0",
    "remix-auth-google": "2.0.0",
    "tailwind-merge": "2.2.1",
    "tailwindcss-animate": "1.0.7",
    "valibot": "0.27.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240222.0",
    "@remix-run/dev": "2.8.1",
    "@remix-run/eslint-config": "2.8.1",
    "@tsconfig/strictest": "2.0.3",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "autoprefixer": "10.4.18",
    "better-sqlite3": "9.4.3",
    "drizzle-kit": "0.20.14",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.14.3",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-unicorn": "51.0.1",
    "eslint-plugin-unused-imports": "3.1.0",
    "markuplint": "4.3.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.2",
    "wrangler": "3.32.0"
  },
  "engines": {
    "node": "20.11.1"
  }
}
